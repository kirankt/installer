{{if .Proxy -}}
[Manager]
{{if .Proxy.HTTPProxy -}}
DefaultEnvironment=HTTP_PROXY="{{.Proxy.HTTPProxy}}" http_proxy="{{.Proxy.HTTPProxy}}"
{{end -}}
{{if .Proxy.HTTPSProxy -}}
DefaultEnvironment=HTTPS_PROXY="{{.Proxy.HTTPSProxy}}" https_proxy="{{.Proxy.HTTPSProxy}}"
{{end -}}
{{if .Proxy.NoProxy -}}
DefaultEnvironment=NO_PROXY="{{.Proxy.NoProxy}}"
{{end -}}
{{end -}}
